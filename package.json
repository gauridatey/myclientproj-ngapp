{
  "name": "myclientproj-ngapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:qa": "ng build --configuration=qa",
    "build:prod": "ng build --prod",
    "start:qa": "ng serve --configuration=qa --port 6200",
    "start:prod": "ng serve --configuration=production --port 5200",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:prettier:check": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{js,json,css}\" -l",
    "format:prettier:fix": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{js,json,css}\" --write",
    "format:html:check": "node bin/glob-ls.js 'src/**/*.html'  | xargs  ./bin/html-beautify-check.sh",
    "format:html:fix": "node bin/glob-ls.js 'src/**/*.html'  | xargs ./node_modules/.bin/html-beautify -r",
    "format:check": "npm run format:prettier:check && npm run format:html:check",
    "format:fix": "npm run format:prettier:fix && npm run format:html:fix",
    "lint:fix": "ng lint --fix",
    "format:lint:fix": "npm run format:fix && npm run lint:fix",
    "compodoc:gen": "./node_modules/.bin/compodoc -p ./tsconfig.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -s ./tsconfig.json",
    "compodoc": "npm run compodoc:gen && npm run compodoc:serve",
    "extract-i18n": "ng xi18n myclientproj-ngapp --format xlf --output-path src/i18n  && ngx-extractor -i src/**/*.ts -o src/i18n/messages.xlf && ng run myclientproj-ngapp:xliffmerge",
    "start-fr": "ng serve --configuration=fr --port 4201 ",
    "start-mr": "ng serve --configuration=mr --port 4202",
    "build:prod-fr": "ng build --configuration=production-fr",
    "build:prod-mr": "ng build --configuration=production-mr",
    "build:qa-fr": "ng build --configuration=qa-fr",
    "build:qa-mr": "ng build --configuration=qa-mr",
    "build:alllangs:prod": "npm run build:prod && npm run build:prod-mr && npm run build:prod-fr",
    "build:alllangs:qa": "npm run build:qa && npm run build:qa-mr && npm run build:qa-fr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "ngx-logger": "^4.1.2",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.10.3",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.19.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check && npm run lint"
    }
  }
}
